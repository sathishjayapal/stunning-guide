{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sathishcovidreportingdatafactory"
		},
		"ls_ablob_sathishcovidreportsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_sathishcovidreportsa'"
		},
		"ls_adls_sathishcovidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_sathishcovidreportingdl'"
		},
		"ls_adls_sathishcovidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sathishcovidreportingdl.dfs.core.windows.net/"
		},
		"ls_http_responsedata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().base_url}"
		},
		"Population File Uploaded_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/80ea84e8-afce-4851-928a-9e2219724c69/resourceGroups/1-9fd188c2-playground-sandbox/providers/Microsoft.Storage/storageAccounts/sathishcovidreportsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sathishcovidreportingdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_sathishcovidreportsa_connectionString": {
						"value": "[parameters('ls_ablob_sathishcovidreportsa_connectionString')]"
					},
					"ls_adls_sathishcovidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_sathishcovidreportingdl_accountKey')]"
					},
					"ls_adls_sathishcovidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_sathishcovidreportingdl_properties_typeProperties_url')]"
					},
					"ls_http_responsedata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_responsedata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"Population File Uploaded_properties_typeProperties_scope": {
						"value": "[parameters('Population File Uploaded_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}